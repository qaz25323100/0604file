#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#define RANDOM_START 1
#define RANDOM_END 11111
int main(){
        int i=0,arr[26];
        FILE *fp;
        char *a;

        fp = fopen("random_text.dat","a");

        /* 產生亂數 */
        for(int i=0;i<26;i++){
                srand( time(NULL) + i);
                int x = (rand()% RANDOM_END)+ 1;
                //fprintf(fp, "%d ",x);
                arr[i]=x;
                fwrite(&arr[i],sizeof(int),1,fp);
        }
        fclose(fp);

        return 0;
}
